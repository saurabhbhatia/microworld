<h2>Ledger Detail </h2>

<table><br/>
<td><th><u> ID </u></th></td>
<td><th><u> Name </u></th></td>
<td><th><u> Amount </u></th></td>
<td><th><u>Feeder Username</u></th></td>
<td><th><u>Date </u></th></td>

<tr>
<% for @ledger in @ledgers %>
<th><td><%= @ledger.id %></th></td>
<th><td><%= @ledger.name %></th></td>
<th><td><%= @ledger.amount %></th></td>
<th><td><%= @ledger.user_name %></th></td>
<th><td><%= @ledger.date_ledger %></th></td>
</tr>
<% end %>
</table>

